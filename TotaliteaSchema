Users(Username, Email, Password, IsAdmin, UserID, Register, Address)
Primary key: UserID
Constraint: check admin value between 0 and 1
Constraint: unique username
Constraint: unique Email

Products(ProductID, ProductName, Description, Category, Supplier, PricePerGram)
Primary key: ProductID


Orders(OrderID, ProductID, UserID, Weight, OrderPlaced)
Composite Primary key: OrderID, ProductID, UserID
Foreign key: ProductID REFERENCE Products(ProductID)
Foreign key: UserID REFERENCES Users(UserID)
